{
    "name": "rougin/combustor",
    "description": "Lets you generate controllers, models, and views from database tables for CodeIgniter",
    "keywords": [
        "codeigniter",
        "combustor",
        "crud",
        "generator",
        "php"
    ],
    "homepage": "https://github.com/rougin/combustor",
    "license": "MIT",
    "authors": [
        {
            "name": "Rougin Royce Gutib",
            "email": "rougingutib@gmail.com",
            "homepage": "http://rougin.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.4.0",
        "rougin/blueprint": "~0.1",
        "rougin/describe": "~1.4",
        "rougin/spark-plug": "~0.4.2"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "rougin/codeigniter": "~3.0"
    },
    "bin": [
        "bin/combustor",
        "bin/combustor.php"
    ],
    "autoload": {
        "psr-4": {
            "Rougin\\Combustor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rougin\\Combustor\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3-dev"
        }
    },
    "suggest": {
        "rougin/codeigniter": "Yet another way to install CodeIgniter via Composer",
        "rougin/refinery": "\"Ready-to-eat\" migrations for Codeigniter"
    }
}
