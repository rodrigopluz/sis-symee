{
    "name": "rodrigopluz/describe",
    "description": "Get the information about the database you're working on in PHP",
    "keywords": ["describe", "database", "php"],
    "homepage": "https://github.com/rodrigopluz/describe",
    "license": "MIT",
    "authors": [
        {
            "name": "Rodrigopluz",
            "email": "rodrigopluz@gmail.com",
            "homepage": "http://rodrigopluz.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "doctrine/inflector": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Rodrigopluz\\Describe\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rodrigopluz\\Describe\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
