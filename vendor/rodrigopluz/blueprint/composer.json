{
    "name": "rodrigopluz/blueprint",
    "description": "Bootstraps your file-generating console applications.",
    "keywords": ["blueprint", "generator", "file-generator", "php"],
    "homepage": "https://github.com/rodrigopluz/blueprint",
    "license": "MIT",
    "authors": [
        {
            "name": "Rodrigopluz",
            "email": "rodrigopluz@gmail.com",
            "homepage": "https://rodrigopluz.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "league/flysystem": "~1.0",
        "rdlowrey/auryn": "~1.0",
        "rodrigopluz/slytherin": "~0.9",
        "symfony/console": "~2.8|~3.0",
        "symfony/yaml": "~2.8|~3.0",
        "twig/twig": "~1.24"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2",
        "scrutinizer/ocular": "~1.1"
    },
    "bin": ["bin/blueprint", "bin/blueprint.php"],
    "autoload": {
        "psr-4": {
            "Rodrigopluz\\Blueprint\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rodrigopluz\\Blueprint\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.5-dev"
        }
    }
}
