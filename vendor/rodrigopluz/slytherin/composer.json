{
    "name": "rodrigopluz/slytherin",
    "description": "Yet another extensible PHP micro-framework.",
    "keywords": ["php framework", "php library", "slytherin"],
    "homepage": "https://github.com/rodrigopluz/slytherin",
    "license": "MIT",
    "authors": [
        {
            "name": "Rodrigopluz",
            "email": "rodrigopluz@gmail.com",
            "homepage": "http://rodrigopluz.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "psr/container": "~1.0",
        "psr/http-message": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Rodrigopluz\\Slytherin\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rodrigopluz\\Slytherin\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}